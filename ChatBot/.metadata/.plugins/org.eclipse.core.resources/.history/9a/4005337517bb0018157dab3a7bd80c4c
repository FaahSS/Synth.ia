package beans;

import java.util.Date;

public class Nivelamento {
	private int codigo;
	private int acerto;
	private String data;
	private String tipo; //dificuldade
	private Usuario codigoUsuario;
	
	//GetAll
	public String getAll() {
		return "Código.............: "+codigo+"\n"+
			   "Número de Acertos..: "+acerto+"\n"+
			   "Usuário............: "+codigoUsuario+"\n"+
			   "Data do Nivelamento: "+data;
	}
	//SetAll
	public void setAll(int codigo, int acerto, Usuario codigoUsuario, Date data) {
		this.codigo = codigo;
		this.acerto = acerto;
		this.codigoUsuario = codigoUsuario;
		this.data = data;
	}
	
	//Construtores
	public Nivelamento() {
		super();
	}
	public Nivelamento(int codigo, int acerto, Usuario codigoUsuario, Date data) {
		super();
		this.codigo = codigo;
		this.acerto = acerto;
		this.codigoUsuario = codigoUsuario;
		this.data = data;
	}
	
	//Getters & Setters
	public int getCodigo() {
		return codigo;
	}
	public void setCodigo(int codigo) {
		this.codigo = codigo;
	}
	public int getAcerto() {
		return acerto;
	}
	public void setAcerto(int acerto) {
		this.acerto = acerto;
	}
	public Usuario getCodigoUsuario() {
		return codigoUsuario;
	}
	public void setCodigoUsuario(Usuario codigoUsuario) {
		this.codigoUsuario = codigoUsuario;
	}
	public String getData() {
		return data;
	}
	public void setData(String data) {
		this.data = data;
	}
	public String getTipo() {
		return tipo;
	}
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	
	
	
	
	
	
}
